{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "factory-ingestion"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureMySqlLab_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureMySqlLab'"
		},
		"Cassandra_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Cassandra'"
		},
		"datalakejethlearn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakejethlearn'"
		},
		"learning_storageblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'learning_storageblob'"
		},
		"AzureKeyVaultLab_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://lab-jeth.vault.azure.net/"
		},
		"Cassandra_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "20.124.90.182"
		},
		"Cassandra_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "cassandra"
		},
		"datalakejethlearn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakejethlearn.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "factory-ingestion_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureMySqlLab_connectionString": {
						"value": "[parameters('AzureMySqlLab_connectionString')]"
					},
					"datalakejethlearn_accountKey": {
						"value": "[parameters('datalakejethlearn_accountKey')]"
					},
					"learning_storageblob_connectionString": {
						"value": "[parameters('learning_storageblob_connectionString')]"
					},
					"Cassandra_password": {
						"value": "[parameters('Cassandra_password')]"
					},
					"AzureKeyVaultLab_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultLab_properties_typeProperties_baseUrl')]"
					},
					"datalakejethlearn_properties_typeProperties_url": {
						"value": "[parameters('datalakejethlearn_properties_typeProperties_url')]"
					},
					"Cassandra_properties_typeProperties_host": {
						"value": "[parameters('Cassandra_properties_typeProperties_host')]"
					},
					"Cassandra_properties_typeProperties_username": {
						"value": "[parameters('Cassandra_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}